{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "msedge",
      "request": "launch",
      "name": "SvelteKit: Debug",
      "url": "http://localhost:5173",
      "webRoot": "${workspaceFolder}",
      "preLaunchTask": "npm: dev",
      "sourceMaps": true,
      "userDataDir": false,
      "runtimeArgs": ["--remote-debugging-port=9222"],
      "breakOnLoad": true,
      "timeout": 30000
    }
  ]
}